{% extends 'base.html.twig' %}

{% block title %}Contact Messages
{% endblock %}

{% block main %}
	<h1>Liste des messages de contact</h1>

	<ul>
		{% for contact in contacts %}
			<li>
				{{contact.name}}
				{{contact.message}}
				{{contact.createdAt|date("m/d/Y")}}</li>

		{% endfor %}
	</ul>


{% endblock %}
